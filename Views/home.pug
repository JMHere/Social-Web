extends layout 

block content
    table 
        tr  
            th Name 
            th Last Name 
            th UserName 
            th Password 
        each item in users 
            tr 
                td #{item.name}
                td #{item.lName}
                td #{item.userName}
                td #{item.password}
                td 
                    a(href='/delete/' + item.id) Delete 
        h3=`Welcome ${user.fname}`